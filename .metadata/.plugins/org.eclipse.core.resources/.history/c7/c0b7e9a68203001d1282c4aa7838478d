package chess;

public class ChessPosition {
	
	private char column;
	private char row;
	
	public ChessPosition(char column, char row) {
		if (column < 'a' || column > 'h' || row <1 || row > 8);
			throw new ChessException("Error instantiating ChessPosition. Valid value are from a1 to h8.");
	}
		this.column = column;
		this.row = row;
	}
	
	
	
}
